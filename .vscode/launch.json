{
	"version": "0.2.0",
	"configurations": [{
			"name": "Listen for XDebug",
			"type": "php",
			"request": "launch",
			"port": 9003,
			"xdebugSettings": {
				"max_children": 1500,
				"max_data": 9048
			},
			"pathMappings": {
				"/var/www/html": "/var/www/html",
			}
		},
		{
			"name": "Launch currently open script",
			"type": "php",
			"request": "launch",
			"program": "${file}",
			"cwd": "${fileDirname}",
			"port": 9003
		}
	]
}
